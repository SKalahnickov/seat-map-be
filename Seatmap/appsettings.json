{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DbConnection": "Server=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};UserName=${DB_USER};Password=${DB_PASSWORD};MaxPoolSize=80;Include Error Detail=true"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      }
    }
  },
  "IntegrationStrings": {
    "BaseUrl": "${EXTERNAL_API_BASE_URL}",
    "GetAttributesUrl": "api/seatmap/get-attributes",
    "SaveUrl": "api/seatmap/save",
    "GetDefaultStructureUrl": "api/seatmap/get-default-structure"
  }
}
